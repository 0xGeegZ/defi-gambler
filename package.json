{
  "name": "@chainlink/box",
  "version": "0.6.0",
  "description": "A Chainlink example in a Truffle box",
  "scripts": {
    "compile": "npx truffle compile --reset",
    "console:dev": "npx truffle console --network cldev",
    "console:bnb:test": "npx truffle console --network binance_testnet",
    "console:kovan": "npx truffle console --network kovan",
    "depcheck": "echo '@chainlink/box' && depcheck --ignore-dirs=build/contracts || true",
    "solhint": "solhint ./contracts/**/*.sol",
    "lint": "yarn solhint",
    "migrate:dev": "npx truffle migrate --reset --network cldev",
    "migrate:kovan": "npx truffle migrate --network kovan",
    "migrate:bnb:test": "npx truffle migrate --network binance_testnet",
    "test:truffle": "npx truffle test",
    "test:mocha": "mocha --recursive './test/*.test.js'",
    "test": "npx hardhat test",
    "coverage": "npx hardhat coverage"
  },
  "license": "MIT",
  "dependencies": {
    "@chainlink/contracts": "^0.2.1",
    "@openzeppelin/contracts": "^4.2.0",
    "@pancakeswap/pancake-swap-lib": "^0.0.4",
    "@truffle/hdwallet-provider": "^1.2.1",
    "@uniswap/lib": "1.1.1",
    "@uniswap/v2-core": "^1.0.1",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.14",
    "solidity-coverage": "^0.7.13",
    "truffle-hdwallet-provider": "^1.0.17"
  },
  "devDependencies": {
    "@chainlink/belt": "^0.0.3",
    "@chainlink/test-helpers": "0.0.5",
    "@ethersproject/bignumber": "^5.4.0",
    "@nomiclabs/buidler": "^1.4.7",
    "@nomiclabs/buidler-ethers": "^2.0.0",
    "@nomiclabs/buidler-truffle5": "^1.3.4",
    "@nomiclabs/buidler-waffle": "^2.0.0",
    "@nomiclabs/buidler-web3": "^1.3.4",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/test-helpers": "^0.5.10",
    "chai": "^4.3.0",
    "depcheck": "^1.3.1",
    "ganache-cli": "^6.12.2",
    "hardhat": "^2.4.1",
    "mocha": "^9.0.1",
    "solhint": "^3.3.2",
    "truffle": "^5.3.1"
  },
  "resolutions": {
    "**/minimist": "^0.2.1",
    "**/node-fetch": "^2.6.1",
    "**/yargs-parser": "^13.1.2",
    "**/mem": "^4.0.0"
  }
}
